    "keywords": ["yii2", "yii 2", "application", "cart", "shop", "module", "pistol88"],
    "homepage": "https://github.com/pistol88/",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/pistol88/yii2-cart"
    },
    "authors": [
        {
            "name": "Ilya Pistol",
            "email": "pistol88@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "autoload": {
        "psr-4": {
            "pistol88\\cart\\": ""
        }
    },
    "extra": {
        "bootstrap": "pistol88\\cart\\Bootstrap"
    }
}
